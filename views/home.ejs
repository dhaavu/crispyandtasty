         <div class="container mt-10" style="font-size: 20px;">
             <div class="col-md-12">
                <center>
                    
                    <div class="panel-group" id="accordion4" role="tablist" aria-multiselectable="true">
                      <% catVal.forEach(function(val, index){ %>
                        <div class="panel panel-default">
                            <div class="panel-heading" role="tab" id="headingOne<%=index%>">
                                <h4 class="panel-title">
                                    <a role="button" data-toggle="collapse" data-parent="#accordion4" href="#collapseOne<%=index%>" aria-expanded="true" aria-controls="collapseOne<%=index%>">
                                        <i class="icon fa fa-plus"></i>
                                        <%=val%>
                                    </a>
                                </h4>
                            </div>
                            <div id="collapseOne<%=index%>" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne<%=index%>">
                                <div class="panel-body">
                                  <div class="row">
                                    <% items.forEach(function(item, index){ %>
                                    <% if(item.cat == val){%>
                                    <!-- Grid column -->
                                    <div class="col-md-6 p-1">

                                      <div class="card danger lighten-2">
                                        <div class="card-body pb-0">
                                        <div class="d-flex justify-content-between">
                                            <p class="mb-0 h5"><%= item.Item %></p>
                                            
                                          </div>
                                        </div>
                                        <hr>
                                        <div class="card-body pt-0 text-left">
                                          <h6 class="font-weight-bold mb-1" style="color:#d11149; font-size: 23px;">Rs.<%=item.Price%>/-</h6>
                                          
                                        </div>
                                      </div>

                                    </div>
                                    <% } %> 
                                    <% });%>
                                    </div>
                                </div>
                            </div>
                        </div>
                      <% });%>
                      
                </center>
            </div>
        </div> 
